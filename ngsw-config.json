{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",           
          "/manifest.webmanifest",
          "/*.css",               
          "/*.js"                 
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch", 
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/img/01.jpg",    
          "/assets/img/02.jpg",
          "/assets/img/03.jpg",
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif)"
        ]
      }
    }
  ]
}